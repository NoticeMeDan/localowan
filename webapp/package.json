{
	"name": "finch-webapp",
	"version": "1.0.0",
	"description": "TTN-Finch Webapp",
	"main": "index.js",
	"scripts": {
		"test": "cross-env start-server-and-test dev http-get://localhost:3000 cypress",
		"lint": "cross-env eslint src config test",
		"lint-fix": "cross-env eslint src config test --fix",
		"profile": "cross-env NODE_ENV=production webpack --config=config/webpack.config.js --profile --json > stats.json",
		"cypress": "cross-env cypress open",
		"build": "cross-env NODE_ENV=production webpack --config=config/webpack.config.js",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --config=config/webpack.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NoticeMeDan/ttn-finch.git"
	},
	"author": "Elias JÃ¸rgensen <ejoe@itu.dk> | Magnus Arnild <mips@itu.dk>",
	"license": "MIT",
	"dependencies": {
		"@acto/ajax": "5.0.3",
		"@date-io/core": "1.3.13",
		"@date-io/dayjs": "1.3.13",
		"@material-ui/core": "4.9.5",
		"@material-ui/icons": "^4.9.1",
		"@material-ui/pickers": "3.2.10",
		"dayjs": "1.8.22",
		"formik": "2.1.4",
		"formik-material-ui": "2.0.0-beta.1",
		"notistack": "0.9.9",
		"react": "16.13.0",
		"react-dom": "16.13.0",
		"react-router-dom": "5.1.2",
		"yup": "0.28.3"
	},
	"devDependencies": {
		"@babel/core": "7.8.7",
		"@babel/preset-env": "7.8.7",
		"@babel/preset-react": "7.8.3",
		"@hot-loader/react-dom": "16.12.0",
		"babel-eslint": "10.1.0",
		"babel-loader": "8.0.6",
		"clean-webpack-plugin": "3.0.0",
		"copy-webpack-plugin": "5.1.1",
		"cross-env": "7.0.2",
		"cypress": "4.2.0",
		"eslint": "6.8.0",
		"eslint-config-standard": "14.1.0",
		"eslint-config-standard-react": "9.2.0",
		"eslint-plugin-cypress": "2.10.3",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-node": "11.0.0",
		"eslint-plugin-promise": "4.2.1",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-standard": "4.0.1",
		"html-webpack-plugin": "3.2.0",
		"prop-types": "15.7.2",
		"react-hot-loader": "4.12.19",
		"start-server-and-test": "1.10.8",
		"webpack": "4.42.0",
		"webpack-cli": "3.3.11",
		"webpack-dev-server": "3.10.3",
		"webpack-merge": "4.2.2",
		"whatwg-fetch": "3.0.0"
	},
	"eslintConfig": {
		"parser": "babel-eslint",
		"extends": [
			"standard",
			"standard-react"
		],
		"rules": {
			"no-tabs": "off",
			"indent": [
				"error",
				"tab",
				{
					"SwitchCase": 1
				}
			],
			"react/jsx-indent": [
				"error",
				"tab"
			],
			"react/jsx-indent-props": [
				"error",
				"tab"
			],
			"react/jsx-closing-bracket-location": [
				1,
				"after-props"
			]
		},
		"plugins": [
			"cypress"
		],
		"env": {
			"browser": true,
			"node": true,
			"cypress/globals": true
		}
	}
}
